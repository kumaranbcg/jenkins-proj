    "components": {
        "schemas": {
            "HardDeletedataRequest": {
                "title": "HardDeletedataRequest",
                "required": ["reportId"],
                "type": "object",
                "properties": {
                    "reportId": {
                        "type": "array",
                        "items": {
                            "type": "integer",
                            "format": "int32"
                        }
                    }
                },
                "example": {
                    "reportId": [1]
                }
            },
            "SubmitCommentRequest": {
                "title": "SubmitCommentRequest",
                "required": ["comment", "replyCommentId", "vcaCommentDocument", "reportId"],
                "type": "object",
                "properties": {
                    "reportId": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "comment": {
                        "type": "string"
                    },
                    "replyCommentId": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "vcaCommentDocument": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "example": {
                    "reportId": 1,
                    "comment": "ss",
                    "replyCommentId": 1,
                    "vcaCommentDocument": []
                }
            },
            "UploadDocRequest": {
                "required": ["file"],
                "type": "object",
                "properties": {
                    "file": {
                        "type": "string",
                        "format": "binary"
                    }
                }
            },
            "GetSignedUrldataRequest": {
                "title": "GetSignedUrldataRequest",
                "required": ["fileKeys"],
                "type": "object",
                "properties": {
